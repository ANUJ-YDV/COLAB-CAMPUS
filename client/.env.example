# ===================================
# COLAB CAMPUS CLIENT - Environment Variables
# ===================================
# Copy this file to .env and fill in your actual values
# IMPORTANT: In Create React App, all variables must start with REACT_APP_
# NEVER commit .env files to Git!

# ===================================
# API CONFIGURATION - DEVELOPMENT
# ===================================
REACT_APP_API_URL=http://localhost:5000
REACT_APP_SOCKET_URL=http://localhost:5000

# ===================================
# API CONFIGURATION - PRODUCTION
# ===================================
# Uncomment and update for production deployment
# REACT_APP_API_URL=https://your-backend-service.onrender.com
# REACT_APP_SOCKET_URL=https://your-backend-service.onrender.com

# ===================================
# FEATURE FLAGS (Optional)
# ===================================
REACT_APP_ENABLE_GITHUB=true
REACT_APP_ENABLE_S3_UPLOAD=true

# ===================================
# ENVIRONMENT INDICATOR
# ===================================
REACT_APP_ENV=development
# Production: REACT_APP_ENV=production
REACT_APP_ENABLE_S3_UPLOAD=true
REACT_APP_ENABLE_COLLABORATIVE_EDITING=true
